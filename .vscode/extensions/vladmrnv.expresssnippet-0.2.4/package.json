{
	"name": "expresssnippet",
	"displayName": "ExpressSnippet",
	"description": "Express Snippets for VS Code",
	"version": "0.2.4",
	"publisher": "vladmrnv",
	"homepage": "https://github.com/vladmrnv/ExpressSnippet",
	"icon": "images/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/vladmrnv/ExpressSnippet"
	},
	"bugs": {
		"url": "https://github.com/vladmrnv/ExpressSnippet/issues"
	},
	"license": "MIT",
	"keywords": [
		"Javascript",
		"Express",
		"Node",
		"Snippet"
	],
	"engines": {
		"vscode": "^0.10.10"
	},
	"categories": [
		"Snippets"
	],
	"activationEvents": [
		"onLanguage:javascript"
	],
	"main": "./out/src/extension",
	"contributes": {
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/express.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^1.8.5",
		"vscode": "^0.11.0"
	},
	"__metadata": {
		"id": "c32bc09b-8525-4d9d-a3da-7f46d8f1858f",
		"publisherId": "524f9945-0e59-4832-a330-a796a3430576",
		"publisherDisplayName": "vladmrnv"
	}
}